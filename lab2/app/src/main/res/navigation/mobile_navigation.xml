<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    app:startDestination="@+id/home_dest">
    <fragment
        android:id="@+id/home_dest"
        android:name="com.example.lab2.HomeFragment"
        android:label="@string/workouts_plural"
        tools:layout="@layout/fragment_home">
        <action
            android:id="@+id/to_sequence"
            app:destination="@+id/sequence_dest">
        </action>
    </fragment>
    <fragment
        android:id="@+id/sequence_dest"
        android:name="com.example.lab2.SequenceFragment"
        android:label="@string/workouts_single"
        tools:layout="@layout/fragment_sequence">
    </fragment>
</navigation>